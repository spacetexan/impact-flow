# Storage backend: 'memory' | 'sqlite' | 'supabase'
VITE_STORAGE_TYPE=memory

# SQLite persistence mode (only applies when VITE_STORAGE_TYPE=sqlite)
# 'memory' = no persistence (resets on refresh)
# 'indexeddb' = persistent storage in browser IndexedDB
VITE_SQLITE_PERSISTENCE=indexeddb

# Enable demo mode with sample data (true | false)
# When true, loads sample profiles/projects on first run
# Set to false for an empty database
VITE_DEMO_MODE=true
